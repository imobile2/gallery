<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
	<link rel="stylesheet" href="http://f.fontdeck.com/s/css/QMzjnQmweVgbuFVAOrRDt3PxX9o/*.intuit.com/12758.css" type="text/css" /> 
	<!-- font for testing, only work for intuit.net  -->
	<link rel="stylesheet" href="http://f.fontdeck.com/s/css/QMzjnQmweVgbuFVAOrRDt3PxX9o/*.intuit.net/12758.css" type="text/css" /> 
</head>
<body>
	<%= render 'layouts/header' %>
    <div class="container">

	  <% flash.each do |key, value| %>
        <!-- <div class="alert alert-<%= key %>"><%= value %></div> -->
		 <%= content_tag(:div, value, class: "alert alert-#{key}") %>
      <% end %>

      <%= yield %>
  	  <%= render 'layouts/footer' %>
	  <%= debug(params) if Rails.env.development? %>
	</div>	
	

</body>
</html>
